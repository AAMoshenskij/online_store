            "pluginVersion": "4.9.0",
            "queryType": "table",
            "rawSql": "SELECT concat_ws(' ', u.first_name, u.last_name) as customer_name, avg(order_total) as average_order_value FROM (SELECT o.user_id as user_id, sum(oi.quantity * p.external_price) as order_total FROM online_store.orders o JOIN online_store.order_items oi ON o.order_id = oi.order_id JOIN online_store.products p ON oi.product_id = p.product_id WHERE $__timeFilter(o.order_date) GROUP BY o.user_id) as order_totals JOIN online_store.users u ON order_totals.user_id = u.user_id GROUP BY u.user_id, u.first_name, u.last_name ORDER BY average_order_value DESC",
